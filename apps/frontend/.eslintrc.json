{
  "extends": [
    "../../.eslintrc.json",
    "next/core-web-vitals",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "import/no-unused-modules": [
      "error",
      {
        "missingExports": true,
        "unusedExports": true,
        "ignoreExports": [
          "**/*.d.ts",
          "**/+(layout|page).tsx"
        ]
      }
    ]
  }
}